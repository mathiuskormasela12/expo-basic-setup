{
  "cli": {
    "version": ">= 5.9.3"
  },
  "build": {
    "development-debug": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "development",
        "EXPO_PUBLIC_APP_NAME": "Unsiagram (Dev)",
        "EXPO_PUBLIC_APP_VERSION": "1.0.0",
        "EXPO_PUBLIC_APP_BUILD_NUMBER": "1",
        "EXPO_PUBLIC_APP_URL": "http://localhost:8080",
        "EXPO_PUBLIC_API_URL": "http://localhost:8080/api/v1"
      }
    },
    "development-release": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "development",
        "EXPO_PUBLIC_APP_NAME": "Unsiagram (Dev)",
        "EXPO_PUBLIC_APP_VERSION": "1.0.0",
        "EXPO_PUBLIC_APP_BUILD_NUMBER": "1",
        "EXPO_PUBLIC_APP_URL": "http://localhost:8080",
        "EXPO_PUBLIC_API_URL": "http://localhost:8080/api/v1"
      }
    },
    "staging-debug": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "staging"
      }
    },
    "staging-release": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "staging"
      }
    },
    "production-debug": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "production"
      }
    },
    "production-release": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}